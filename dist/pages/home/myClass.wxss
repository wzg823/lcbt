/* pages/home/myClass.wxss */

.title-view {
    color: #848296;
    padding: 30rpx;
}
.bottom-view {
    color: #BAB9C7;
    padding: 15rpx 0;
}
.up-view {
    border-bottom: 1rpx #262439 solid;
    padding: 20rpx 0 15rpx;
}
.up-view .headimg{
    width: 130rpx;
    height: 130rpx;
    margin-right: 30rpx;
    float: left;
    margin-top: 11rpx;
}
.info-view .name-text{
    font-size: 34rpx;
    margin-bottom: 30rpx;
    color: white;
}

.myclass-cell {
    margin-bottom: 20rpx;
    padding: 0 30rpx 0;
    color: #BAB9C7;
    background-color: #22233A;
    font-size: 28rpx;
    height: 250rpx;
}
.btn {
    width: 150rpx;
    height: 50rpx;
    line-height: 50rpx;
    color: #FF904C;
    border: 1rpx #FF904C solid;
    border-radius: 6rpx;
    text-align: center;
    float: right;
    margin-top: 80rpx;
}
.cancel {
    color: #BAB9C7;
    border: 1rpx #BAB9C7 solid;
}